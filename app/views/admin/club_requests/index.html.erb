<table class="responstable">
  <tr>
    <th>Request name</th>
    <th>Requester</th>
    <th>Request type</th>
    <th>Request create at</th>
    <th>Action</th>
  </tr>
  <% @club_requests.each do |club_request|%>
    <tr>
      <td><%= club_request.name %></td>
      <td><%= club_request.user.full_name %></td>
      <td><%= club_request.action %></td>
      <td><%= l club_request.created_at, format: :date %></td>
      <td>
        <%= link_to [:admin, club_request], method: :delete, data: {confirm: "confirm_delete"} do %>
          <span class="glyphicon glyphicon-trash" title="Delete"></span>
        <% end %>
        <%= link_to edit_admin_club_request_path(club_request) do %>
          <span class="glyphicon glyphicon-edit right" title="Edit"></span>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>
